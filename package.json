{
  "name": "al-db-schema",
  "displayName": "AL DB Schema",
  "description": "Generate DB schemas from yours Business Central AL projects inside VSCode",
  "repository": {"type": "git", "url": "https://github.com/remi-vidal-michel/al-db-schema"},
  "icon": "images/icon.png",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "al-db-schema.generateSchema",
        "title": "AL DB Schema: Generate Database Schema"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
